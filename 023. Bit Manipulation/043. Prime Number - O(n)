
#include <iostream>
using namespace std;

void isPrime(int n)
{

    int count = 0;
    for (int i = 1; i <= n; i++)
    {
        if (n % i == 0)
            count++;
    }

    if (count == 2)
        cout << "YES ";
    else
        cout << "NO";
}

int main()
{
    int n = 20;
    isPrime(n);
}

// output
// NO
