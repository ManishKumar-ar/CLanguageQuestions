
#include <iostream>
#include <vector>
using namespace std;

int main()
{
    vector<int> vec;
    vec.push_back(10);
    vec.push_back(20);
    vec.push_back(30);
    vec.push_back(40);

   
    vector <int> vec2(vec.begin(),vec.begin()+2);         // not including, the element at the position pointed to by vec.begin() + 2.

      for (int i = 0; i < vec2.size(); ++i) {
        cout << vec2[i] << " ";
    }
}

//output 
// 10 20 
