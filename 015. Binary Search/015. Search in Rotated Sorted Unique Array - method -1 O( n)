#include <iostream>
#include <vector>
using namespace std;
int search(vector<int>& arr, int n, int k)
{
    for( int i =0;i<n ;i++)
    {
        if(arr[i]==k)
        return i;
    }
    return -1;
}

int main()
{
    vector<int > vec = {12, 15, 18, 2, 4};
    int result = search(vec, 5,2);
    cout<<"At index: "<<result;
}

//output
// At index: 3
