#include <iostream>
using namespace std;
int main()
{
    for (int i = 5; i >= 1; i--)
    {
        for (int j = 1; j <= i; j++)
        {
            cout<<j;
        }
        cout<<endl;
    }
}

//output
// 12345
// 1234
// 123
// 12
// 1






.................................................................................................................
#include <iostream>
using namespace std;
int main()
{
    int N = 5;
    for (int i = 0; i < N; i++)
    {
         
        for (int j = N; j>i; j--)
        {
            cout <<N-j+1<<" ";
        }
        cout << endl;
    }
}

//output

// 1 2 3 4 5 
// 1 2 3 4 
// 1 2 3 
// 1 2 
// 1 
// 1
