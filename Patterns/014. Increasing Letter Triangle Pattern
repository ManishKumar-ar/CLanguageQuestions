#include <iostream>
using namespace std;
void pattern(int n)
{
    char num = 'A';

    for (int i = 0; i < n; i++)
    {
        char num = 'A';

        for (int j = 0; j <= i; j++)
        {
            cout << num<<" ";
            num++;
        }

        cout << endl;
    }
}
int main()
{
    pattern(5);
}

// output
// A 
// A B
// A B C
// A B C D
// A B C D E
