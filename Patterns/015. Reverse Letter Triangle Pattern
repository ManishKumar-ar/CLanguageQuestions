#include <iostream>
using namespace std;
void pattern(int n)
{
   

    for (int i = n; i >=1; i--)
    {
        char num = 'A';

        for (int j = 1; j <=i; j++)
        {
            cout << num<<" ";
            num++;
        }

        cout << endl;
    }
}
int main()
{
    pattern(5);
}

// output
// A B C D E 
// A B C D
// A B C
// A B
// A
