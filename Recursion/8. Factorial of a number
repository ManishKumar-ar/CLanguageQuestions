#include<iostream>
using namespace std;

// the time complexity  - O(n)
// the space complexity  - O(n)                    /// because we use recursion here, so for every call there si activation record in the  stack


int  fun(int n)
{
    if(n==1)
    return 1;
    
    return n*fun(n-1);

}

int main()
{
    int n=5;
  int result =  fun(n);
  cout<<result;
}

//output 
//120
