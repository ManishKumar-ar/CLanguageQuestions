#include <iostream>
using namespace std;

int main() {
    int m = 2;
    int n = 6;

    int result = 1; // Initialize the result variable to 1

    while (n > 0) { // Changed to n > 0
        if (n % 2 == 1) { // If n is odd
            result *= m;
        }
        m *= m; // Square the base
        n /= 2; // Halve the exponent
    }

    cout << result << endl; // Print the result
    return 0;
}


//output  -----------------------------------------
32
