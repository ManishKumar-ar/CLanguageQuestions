#include <iostream>
#include <map>
using namespace std;

int main()
{
    int arr[] = {1, 20, 3, 1, 3, 2, 12};
    map<int, int> mp;
    for (int i = 0; i < 7; i++)
    {
        mp[arr[i]]++;
    }

    for (int i = 0; i < 7; i++)
    {
        cout << arr[i] << " ";
    }

    cout << endl;

    for (auto pair : mp)
    {
        cout << "Element: " << pair.first << " --> " << pair.second << endl;
    }
}

//output 
// 1 20 3 1 3 2 12 
// Element: 1 --> 2
// Element: 2 --> 1
// Element: 3 --> 2
// Element: 12 --> 1
// Element: 20 --> 1
