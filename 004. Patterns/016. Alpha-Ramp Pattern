#include <iostream>
using namespace std;
void pattern(int n)
{

    for (int i = 0; i < n; i++)
    {
        char num = 'A';
        num += i;

        for (int j = 0; j <= i; j++)
        {
            cout << num << " ";
        }

        cout << endl;
    }
}
int main()
{
    pattern(5);
}

// output
// A 
// B B
// C C C
// D D D D
// E E E E E
