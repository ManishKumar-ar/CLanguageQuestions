#include <iostream>
using namespace std;
int main()
{
    int key;

    int a[5], n = 5;

    cout << "enter " << n << " numbers";

    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
    }
    cout << "entered elements are -\n";
    for (int i = 0; i < n; i++)
    {
        cout << a[i] << " ";
    }

    cout << "enter key which you want to find ";
    cin >> key;

    for (int i = 0; i < n; i++)
    {
        if (key == a[i])
        {
            cout << "found at index " << i;
            return 0;
        }
    }
    cout << "key is not found ";
}


output ---
enter 5 numbers10 20 304 78 98
entered elements are -
10 20 304 78 98 enter key which you want to find 98
found at index4
