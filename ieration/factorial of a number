
#include <iostream>
using namespace std;
int factorial(int x)
{
    int fact = 1;
    if (x == 1 || x == 0)
        return 1;
    else
    {
        ;
        for (int i = 1; i <= x; i++)
        {

            fact *= i;
        }
    }
    return fact;
}
int main()
{
    int result = factorial(5);
    cout << result;
}
