#include <iostream>
using namespace std;
int main()
{
    int m, n;
    cout << "Enter two no.s " << endl;
    cin >> m >> n;
    while (m != n)
    {
        if (m > n)
            m = m - n;
        else
            n = n - m;
    }
    cout << "GCD is " << m;
    return 0;
}

//// output -----
Enter two no.s
15
9
GCD is 3


//// output -----
Enter two no.s 
17
89
GCD is 1
