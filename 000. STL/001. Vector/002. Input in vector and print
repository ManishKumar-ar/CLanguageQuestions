#include <iostream>
#include <vector>
using namespace std;

void printVecotor(vector<int> v)
{
    for (int i = 0; i < v.size(); i++)
    {
        cout << v[i] << " ";
    }
    cout << endl;
}

int main()
{
    vector<int> v;

    cout << "Enter the size of the vector ";

    int n;
    cin >> n;

    for (int i = 0; i < n; i++)
    {
        int x;
        cin >> x;
        v.push_back(x);
    }

    printVecotor(v);
}


//output 
// Enter the size of the vector 5
// 10
// 20
// 30
// 80
// 90
// 10 20 30 80 90 
