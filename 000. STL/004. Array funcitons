
#include <iostream>
#include <array>
using namespace std;

int main()
{

    array<int, 5> arr = {1, 3, 4, 5, 6};

    cout << arr.size() << endl; // give the size of the array

    cout << arr.front() << endl; // give the first element of the array

    cout << arr.back() << endl; // give the last element of the array
}

// output
// 5
// 1
// 6
