#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
    vector<int> vec = {10, 5, 6, 100, 99};

    sort(vec.begin(), vec.begin()+4 );         // this is provied by the algorithm header file

   
    for (auto it = vec.begin(); it != vec.end(); ++it)
    {
        cout << *it << " ";
    }
    cout<<endl;

}

//output
// 5 6 10 100 99 
