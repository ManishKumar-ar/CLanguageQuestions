#include <iostream>
#include <numeric>         // here we use this header file
using namespace std;

int main() {
    int arr[] = {10, 5, 6, 88, 99};

    // Finding the sum of elements in the array
    int sum = accumulate(begin(arr), end(arr), 0);

   
    cout << "Sum of elements: " << sum << endl;

    return 0;
}

//output
// Sum of elements: 208


//note -
//accumulate( first_index, Last_index, initial_sum);
//time compelxity -
//standard containers like arrays and vectors, the time complexity is typically linear O( n )
