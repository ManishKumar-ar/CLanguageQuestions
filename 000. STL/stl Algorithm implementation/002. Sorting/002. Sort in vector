#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
    vector<int> vec = {10, 5, 6, 88, 99};

    sort(vec.begin(), vec.end());         // this is provied by the algorithm header file

    for (int num : vec)
        cout << num << " ";

        
    cout<<endl;

    for (size_t i = 0; i < vec.size(); ++i)
    {
        cout << vec[i] << " ";
    }
    cout<<endl;

    for (auto it = vec.begin(); it != vec.end(); ++it)
    {
        cout << *it << " ";
    }
    cout<<endl;

}
