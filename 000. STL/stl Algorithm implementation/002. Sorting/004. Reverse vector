#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    vector<int> vec = {10, 5, 6, 88, 99};

    
    sort(begin(vec), end(vec));

   
    reverse(begin(vec), end(vec));

   
    for (int num : vec) {
        cout << num << " ";
    }
    
    cout << endl;

    return 0;
}

//output
99 88 10 6 5



...........................
//another way


#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    vector<int> vec = {10, 5, 6, 88, 99};

     sort(vec.begin(), vec.end());

    reverse(vec.begin(), vec.end());

    for (int num : vec) {
        cout << num << " ";
    }
    
    cout << endl;

    return 0;
}

//output
// 99 88 10 6 5 
