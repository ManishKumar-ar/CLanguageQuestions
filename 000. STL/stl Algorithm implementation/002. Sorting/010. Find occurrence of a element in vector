#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    vector<int> vec = {1, 2, 3, 4, 3, 2, 1, 2, 3, 4};
    int target = 3; 

                                                      // Counting the occurrences of the target element in the vector
    int count = std::count(vec.begin(), vec.end(), target);

    cout << "The element " << target << " appears " << count << " times in the vector." << endl;

}

//ouput
The element 3 appears 3 times in the vector.
