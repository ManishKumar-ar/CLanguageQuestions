
#include <iostream>
using namespace std;

int Array(int A[], int leng)
{
    int i=0;
    int j= leng-1;
    while(i<j)
    {
        while(A[i]<0)
        {i++;}
        while(A[j]>=0){j--;}
        if(i<j)
        {
            int temp;
            temp=A[i];
            A[i]=A[j];
            A[j]= temp;
        }

    }
}

int main()
{
    int arr[10] = {-6,3,-8,10,5,-7,-9,12,-4,2};
    Array(arr, 10);
    
    
    for(int i=0;i<10;i++)
    {
        cout<< arr[i]<<" ";
    }
   
}


//output 
// -6 -4 -8 -9 -7 5 10 12 3 2
