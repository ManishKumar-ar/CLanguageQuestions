#include <iostream>
#include <string>
using namespace std;

int find(string s, char q)
{
    int cnt = 0;
    for (int i = 0; i < s.length(); i++)
    {
        if (s[i] == q)
            cnt++;
    }

    return cnt;
}

int main()
{
    string s = "abcdaaabcde";
    char q = 'a';
    int cnt = find(s, q);
    cout << q << " occures in the array at " << cnt << " times";
}


//output 
//a occures in the array at 4 times
