
#include <iostream>
#include <vector>
#include <map>

using namespace std;

vector<int> majorityElement(vector<int> &nums)
{
    int n = nums.size();
    int mini = (n / 3) + 1;

    vector<int> ans;

    map<int, int> h;

    for (int i = 0; i < n; i++)
    {
        h[nums[i]]++;
        if (h[nums[i]] == mini)
        {
            ans.push_back(nums[i]);
        }
    }

    return ans;
}
int main()
{
    vector<int> vec = {1, 1, 1, 2, 2, 2, 3, 3};
    vector<int> result = majorityElement(vec);

    for (auto it : result)
    {
        cout << it << " ";
    }
}

// output
//  1 2
