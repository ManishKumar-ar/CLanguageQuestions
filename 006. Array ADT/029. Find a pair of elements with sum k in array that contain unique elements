// here the time complexity is O ( n^2 )

#include <iostream>
using namespace std;

int main()
{
    int A[10] = {6, 3, 8, 10, 16, 7, 5, 2, 9, 14};
    int k=10;

    for (int i = 0; i <= 8; i++)
    {
        for (int j = i + 1; j < 10; j++)
        {
            if (A[i] + A[j] == k)
            {
                cout<<A[i]<<" + "<<A[j]<<" = "<<" 10 "<<endl;
            }
        }
    }
}


//output 
// 3 + 7 =  10 
// 8 + 2 =  10
